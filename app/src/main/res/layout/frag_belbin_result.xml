<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Baggrundsfarve">


        <com.github.mikephil.charting.charts.RadarChart
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:id="@+id/spiderweb">
        </com.github.mikephil.charting.charts.RadarChart>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_marginLeft="10dp"
                    android:layout_marginBottom="0dp">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                                <LinearLayout
                                    android:orientation="vertical"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                        <TextView
                                            android:text="IdÃ©mand:"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/textIdemand"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:textStyle="bold" />

                                        <TextView
                                            android:text="Kontaktskaber:"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/textKontaktskaber"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:textStyle="bold" />

                                        <TextView
                                            android:text="Koordinator:"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/textKoordinator"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:textStyle="bold" />

                                        <TextView
                                            android:text="Opstarter:"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/textOpstarter"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:textStyle="bold" />

                                        <TextView
                                            android:text="Analytiker:"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/textAnalytiker"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:textStyle="bold" />
                                </LinearLayout>

                                <LinearLayout
                                    android:orientation="vertical"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1.5">

                                        <TextView
                                            android:text="TextView"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/resIde"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:layout_gravity="left" />

                                        <TextView
                                            android:text="TextView"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/resKon"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:layout_gravity="left" />

                                        <TextView
                                            android:text="TextView"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/resKoo"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:layout_gravity="left" />

                                        <TextView
                                            android:text="TextView"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/resOps"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:layout_gravity="left" />

                                        <TextView
                                            android:text="TextView"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/resAna"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:layout_gravity="left" />
                                </LinearLayout>

                        </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_marginLeft="0dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="0dp">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                                <LinearLayout
                                    android:orientation="vertical"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                        <TextView
                                            android:text="Formidler:"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/textFormidler"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:textStyle="bold" />

                                        <TextView
                                            android:text="Organisator:"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/textOrganisator"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:textStyle="bold" />

                                        <TextView
                                            android:text="Afslutter:"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/textAfslutter"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:textStyle="bold" />

                                        <TextView
                                            android:text="Specialist:"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/textSpecialist"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:textStyle="bold" />

                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            app:srcCompat="@color/Baggrundsfarve"
                                            android:id="@+id/imageView3"
                                            android:layout_weight="1" />

                                </LinearLayout>

                                <LinearLayout
                                    android:orientation="vertical"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1.5">

                                        <TextView
                                            android:text="TextView"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/resFor"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:layout_gravity="left" />

                                        <TextView
                                            android:text="TextView"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/resOrg"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:layout_gravity="left" />

                                        <TextView
                                            android:text="TextView"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/resAfs"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:layout_gravity="left" />

                                        <TextView
                                            android:text="TextView"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:id="@+id/resSpe"
                                            android:layout_weight="1"
                                            android:textColor="@color/white"
                                            android:layout_gravity="left" />

                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            app:srcCompat="@color/Baggrundsfarve"
                                            android:id="@+id/imageView2"
                                            android:layout_weight="1" />

                                </LinearLayout>

                        </LinearLayout>

                </LinearLayout>
        </LinearLayout>


</LinearLayout>